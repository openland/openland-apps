{
    "env": {
        "default": {
            "presets": ["next/babel", "@zeit/next-typescript/babel", "linaria/babel"],
            "plugins": [
                "graphql-tag",
                [
                    "module-resolver",
                    {
                        "root": ["./packages/"]
                        // "alias": {
                        //     "openland-icons": "openland-icons"
                        // }
                    }
                ],
                "inline-react-svg",
                "transform-react-constant-elements",
                "transform-react-remove-prop-types",
                "glamorous-displayname",
                [
                    "react-native-web",
                    {
                        "commonjs": true
                    }
                ]
            ]
        },
        "webproduction": {
            "presets": ["next/babel", "@zeit/next-typescript/babel", "linaria/babel"],
            "plugins": [
                "graphql-tag",
                [
                    "module-resolver",
                    {
                        "root": ["./packages/"]
                        // "alias": {
                        //     "openland-icons": "openland-icons"
                        // }
                    }
                ],
                "transform-mental",
                "inline-react-svg",
                "transform-react-constant-elements",
                "transform-react-remove-prop-types",
                "glamorous-displayname",
                [
                    "react-native-web",
                    {
                        "commonjs": true
                    }
                ]
            ]
        },
        "production": {
            "presets": ["module:metro-react-native-babel-preset"],
            "plugins": [
                "graphql-tag",
                [
                    "module-resolver",
                    {
                        "root": ["./build/native"],
                        "alias": {
                            "openland-mobile": "./build/native/openland-mobile",
                            "react-native-s": "./build/native/react-native-s",
                            "react-native-async-view": "./build/native/react-native-async-view",
                            "react-native-notification-handler": "./build/native/react-native-notification-handler",
                            "openland-video": "./build/native/openland-video",
                            "openland-api": "./build/native/openland-api",
                            "openland-graphql": "./build/native/openland-graphql",
                            "openland-y-graphql": "./build/native/openland-y-graphql",
                            "openland-engines": "./build/native/openland-engines",
                            "openland-y-utils": "./build/native/openland-y-utils",
                            "openland-y-runtime": "./build/native/openland-y-runtime-native",
                            "openland-y-runtime-native": "./build/native/openland-y-runtime-native",
                            "openland-y-runtime-api": "./build/native/openland-y-runtime-api",
                            "openland-y-forms": "./build/native/openland-y-forms",
                            "openland-xp": "./build/native/openland-xp",
                            "openland-shared": "./build/native/openland-shared",
                            "openland-form": "./build/native/openland-form",
                            "assets": "./packages/openland-mobile/assets",
                            "assets-s": "./packages/react-native-s/assets",
                            "openland-pegasus": "./build/native/openland-pegasus",
                            "openland-powerups": "./build/native/openland-powerups"
                        }
                    }
                ],
                "transform-remove-console",
                // "@babel/plugin-transform-react-inline-elements",
                // "@babel/plugin-transform-react-constant-elements",
                "./packages/react-native-async-view-babel/lib"
            ]
        },
        "development": {
            "presets": ["module:metro-react-native-babel-preset"],
            "plugins": [
                [
                    "module-resolver",
                    {
                        "root": ["."],
                        "alias": {
                            "openland-mobile": "./build/native/openland-mobile",
                            "react-native-s": "./build/native/react-native-s",
                            "react-native-async-view": "./build/native/react-native-async-view",
                            "react-native-notification-handler": "./build/native/react-native-notification-handler",
                            "openland-api": "./build/native/openland-api",
                            "openland-video": "./build/native/openland-video",
                            "openland-graphql": "./build/native/openland-graphql",
                            "openland-y-graphql": "./build/native/openland-y-graphql",
                            "openland-engines": "./build/native/openland-engines",
                            "openland-y-utils": "./build/native/openland-y-utils",
                            "openland-y-runtime": "./build/native/openland-y-runtime-native",
                            "openland-y-runtime-native": "./build/native/openland-y-runtime-native",
                            "openland-y-runtime-api": "./build/native/openland-y-runtime-api",
                            "openland-y-forms": "./build/native/openland-y-forms",
                            "openland-y-store": "./build/native/openland-y-store",
                            "openland-xp": "./build/native/openland-xp",
                            "openland-shared": "./build/native/openland-shared",
                            "openland-form": "./build/native/openland-form",
                            "assets": "./packages/openland-mobile/assets",
                            "assets-s": "./packages/react-native-s/assets",
                            "openland-pegasus": "./build/native/openland-pegasus",
                            "openland-powerups": "./build/native/openland-powerups"
                        }
                    }
                ],
                // "@babel/plugin-transform-react-constant-elements",
                "./packages/react-native-async-view-babel/lib"
                // "@babel/plugin-transform-react-inline-elements"
            ]
        }
    }
}
