{
    "env": {
        "default": {
            "presets": ["next/babel", "@zeit/next-typescript/babel"],
            "plugins": ["transform-react-stateless-component-name", "inline-react-svg", ["react-native-web", {
                "commonjs": true
            }]]
        },
        "production": {
            "presets": ["react-native"],
            "plugins": [
                ["module-resolver", {
                    "root": ["./build/native"],
                    "alias": {
                        "react-native-s": "./build/native/react-native-s",
                        "react-native-async-view": "./build/native/react-native-async-view",
                        "react-native-fast-navigation": "./build/native/react-native-fast-navigation",
                        "openland-api": "./build/native/openland-api",
                        "openland-y-graphql": "./build/native/openland-y-graphql",
                        "openland-engines": "./build/native/openland-engines",
                        "openland-y-utils": "./build/native/openland-y-utils",
                        "openland-y-runtime": "./build/native/openland-y-runtime-native",
                        "openland-y-runtime-native": "./build/native/openland-y-runtime-native",
                        "openland-y-runtime-api": "./build/native/openland-y-runtime-api",
                        "openland-y-forms": "./build/native/openland-y-forms",
                        "openland-xp": "./build/native/openland-xp",
                        "openland-shared": "./build/native/openland-shared",
                        "assets": "./packages/openland-mobile/assets"
                    }
                }],
                "transform-remove-console"
            ]
        },
        "development": {
            "presets": ["react-native"],
            "plugins": [
                ["module-resolver", {
                    "root": ["."],
                    "alias": {
                        "react-native-s": "./build/native/react-native-s",
                        "react-native-async-view": "./build/native/react-native-async-view",
                        "react-native-fast-navigation": "./build/native/react-native-fast-navigation",
                        "openland-api": "./build/native/openland-api",
                        "openland-y-graphql": "./build/native/openland-y-graphql",
                        "openland-engines": "./build/native/openland-engines",
                        "openland-y-utils": "./build/native/openland-y-utils",
                        "openland-y-runtime": "./build/native/openland-y-runtime-native",
                        "openland-y-runtime-native": "./build/native/openland-y-runtime-native",
                        "openland-y-runtime-api": "./build/native/openland-y-runtime-api",
                        "openland-y-forms": "./build/native/openland-y-forms",
                        "openland-y-store": "./build/native/openland-y-store",
                        "openland-xp": "./build/native/openland-xp",
                        "openland-shared": "./build/native/openland-shared",
                        "assets": "./packages/openland-mobile/assets"
                    }
                }]
            ]
        }
    }
}