	"scripts": {
		"sources": "./node_modules/typescript/bin/tsc",
		"sources:watch": "./node_modules/typescript/bin/tsc --watch",
		"less": "yarn less-watch-compiler --run-once src/less build/dist/static/css style.less",
		"less:watch": "yarn less-watch-compiler --minified false src/less build/dist/static/css style.less",
		"assets": "yarn cpx \"./src/static/**/*\" ./build/dist/static/",
		"assets:watch": "yarn cpx \"./src/static/**/*\" ./build/dist/static/ --watch",
		"bundle": "yarn sources && yarn assets && yarn less",
		"bundle:watch": "concurrently \"yarn sources:watch\" \"yarn assets:watch\" \"yarn less:watch\"",
		"prebuild": "NODE_ENV=production yarn bundle",
		"build": "NODE_ENV=production yarn next build ./build/dist",
		"server": "node ./build/dist/server.js",
		"predev": "yarn bundle",
		"dev:local": "concurrently \"yarn sources:watch\" \"yarn assets:watch\" \"yarn less:watch\" \"yarn server\"",
		"dev:staging": "API_ENDPOINT=\"https://api.datamakesperfect.com\" yarn dev:local",
		"dev": "API_ENDPOINT=\"https://api.statecrafthq.com\" yarn dev:local",
		"analyze": "yarn build && cross-env ANALYZE=1 next build ./build/dist",
		"start": "NODE_ENV=production yarn server",
		"heroku-postbuild": "yarn build",
		"promote": "kubectl set image deployment/statecraft-web statecraft-web=`kubectl get deployment statecraft-web-staging -o=jsonpath='{$.spec.template.spec.containers[:1].image}'` && kubectl rollout status deployments statecraft-web",
		"schema": "yarn schema:download && yarn schema:generate",
		"schema:local": "yarn schema:download:local && yarn schema:generate",
		"schema:download": "yarn apollo-codegen introspect-schema https://api.statecrafthq.com/api --output schema.json",
		"schema:download:local": "yarn apollo-codegen introspect-schema http://localhost:9000/api --output schema.json",
		"schema:generate": "yarn apollo-codegen generate ./src/api/queries/*.ts --schema schema.json --target typescript --output ./src/api/Types.ts --addTypename --tag-name gql"
	}
}
