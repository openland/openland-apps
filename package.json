    "scripts": {
        "format-web": "prettier --write \"./packages/openland-web/**/*.tsx\" \"./packages/openland-web/**/*.ts\" \"./packages/openland-web/**/*.js\"",
        "dev": "BABEL_ENV=default API_WS_ENDPOINT=\"wss://api.openland.com/api\" API_ENDPOINT=\"https://api.openland.com\" nodemon ./packages/openland-web/server.ts",
        "prod-local:start": "BABEL_ENV=default NODE_ENV=production API_WS_ENDPOINT=\"wss://api.openland.com/api\" API_ENDPOINT=\"https://api.openland.com\" nodemon ./dist/server.js",
        "prod:start": "BABEL_ENV=default NODE_ENV=production API_WS_ENDPOINT=\"wss://api.openland.com/api\" API_ENDPOINT=\"https://api.openland.com\" nodemon ./packages/openland-web/server.ts",
        "prod": "yarn bundle && yarn prod:start",
        "dev:ngrok": "ngrok http 3000 -subdomain=openweb --region=eu",
        "dev:local": "BABEL_ENV=default API_WS_ENDPOINT=\"ws://localhost:9000/api\" nodemon ./packages/openland-web/server.ts",
        "lint": "BABEL_ENV=default NODE_ENV=production ./node_modules/.bin/tslint --project . --config ./tslint.json",
        "jest": "BABEL_ENV=test node ./node_modules/jest/bin/jest.js --no-cache --testPathIgnorePatterns openland-e2e-perf",
        "jest:watch": "BABEL_ENV=test node ./node_modules/jest/bin/jest.js --testPathIgnorePatterns openland-e2e-perf --watch",
        "test": "./node_modules/.bin/concurrently \"yarn lint\" \"yarn jest\" \"yarn test:ts\"",
        "test:ts": "BABEL_ENV=default NODE_ENV=production ./node_modules/.bin/tsc -p tsconfig.json --noemit",
        "codemod:build": "BABEL_ENV=default NODE_ENV=production ./node_modules/.bin/tsc -p tsconfig.codemod.json",
        "bundle:app": "BABEL_ENV=webproduction NODE_ENV=production next build ./packages/openland-web",
        "bundle:server": "BABEL_ENV=webproduction NODE_ENV=production ./node_modules/.bin/tsc -p tsconfig.server.json",
        "bundle:prepare": "./node_modules/.bin/cpx \"./packages/openland-web/next.config.prod.js\" ./dist/ && ./node_modules/.bin/cpx \"./packages/openland-web/worker.js\" ./dist/ && mv ./dist/next.config.prod.js ./dist/next.config.js  && ./node_modules/.bin/cpx \"./packages/openland-web/.next/**/*\" ./dist/.next/ && ./node_modules/.bin/cpx \"./packages/openland-web/static/**/*\" ./dist/static/",
        "bundle": "./node_modules/.bin/concurrently \"yarn bundle:server\" \"yarn bundle:app\" && yarn bundle:prepare",
        "analyze": "BABEL_ENV=default NODE_ENV=production BUNDLE_ANALYZE=both next build ./packages/openland-web",
        "analyze:web": "BABEL_ENV=default NODE_ENV=production BUNDLE_ANALYZE=browser next build ./packages/openland-web",
        "analyze:server": "BABEL_ENV=default NODE_ENV=production BUNDLE_ANALYZE=server next build ./packages/openland-web",
        "inspect:build": "BABEL_ENV=default NODE_ENV=production next build ./packages/openland-web --inspect",
        "inspect:dev": "BABEL_ENV=default TS_NODE_PROJECT=tsconfig.server.json node --inspect -r ts-node/register ./packages/openland-web/server.ts",
        "native": "./node_modules/.bin/tsc -p tsconfig.json --outDir ./build/native && ./node_modules/.bin/concurrently \"yarn native:metro\" \"yarn native:build:watch\"",
        "native:metro": "node node_modules/react-native/local-cli/cli.js start",
        "native:build": "./node_modules/.bin/tsc -p tsconfig.json --jsx react-native --outDir ./build/native",
        "native:build:watch": "./node_modules/.bin/tsc -w -p tsconfig.json --jsx react-native --outDir ./build/native",
        "native:ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator=\"iPhone X\"",
        "native:bundle": "./scripts/native_bundle.sh",
        "native:ios:logs": "node node_modules/react-native/local-cli/cli.js log-ios",
        "native:android": "node node_modules/react-native/local-cli/cli.js run-android",
        "native:ngrok": "ngrok http 8081 -subdomain=openland --region=eu",
        "native:adb": "adb reverse tcp:8081 tcp:8081",
        "native:res": "node ./packages/openland-mobile/assets/sources/_extract.js",
        "native:babel": "yarn tsc --project ./packages/react-native-async-view-babel",
        "schema": "yarn schema:download && yarn schema:generate",
        "schema:local": "yarn schema:download:local && yarn schema:generate",
        "schema:download": "./scripts/schema_download.sh",
        "schema:download:local": "./scripts/schema_download_local.sh",
        "schema:generate": "./scripts/schema_generate.sh",
        "favicon-gen": "convert ./packages/openland-web/static/favicon-16.png ./packages/openland-web/static/favicon-32.png ./packages/openland-web/static/favicon-48.png ./packages/openland-web/static/favicon.ico",
        "postinstall": "patch-package",
        "mac": "./node_modules/.bin/concurrently \"yarn mac:assets\" \"yarn mac:build:watch\" \"yarn mac:bundler\"",
        "mac:start": "cd ./packages/openland-mac && node node_modules/react-native-macos/local-cli/cli.js run-macos",
        "mac:assets": "./node_modules/.bin/cpx \"./packages/openland-mobile/assets/**/*\" ./packages/openland-mac/build/assets",
        "mac:build": "./node_modules/.bin/tsc -p tsconfig.json --outDir ./packages/openland-mac/build/native",
        "mac:build:watch": "./node_modules/.bin/tsc -w -p tsconfig.json --preserveWatchOutput --outDir ./packages/openland-mac/build/native",
        "mac:bundler": "./scripts/mac_bundler.sh",
        "mac:prepare": "cd ./packages/openland-mac && yarn"
    }
