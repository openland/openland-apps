	"scripts": {
		"dev": "API_ENDPOINT=\"https://api.statecrafthq.com\" nodemon ./packages/openland-web/server.ts",
		"dev:local": "nodemon ./packages/openland-web/server.ts",
		"lint": "NODE_ENV=production ./node_modules/.bin/tslint --project . --config ./tslint.json",
		"compile": "NODE_ENV=production ./node_modules/.bin/tsc",
		"test": "yarn lint && yarn compile",
		"build": "NODE_ENV=production next build ./packages/openland-web",
		"analyze": "BUNDLE_ANALYZE=both next build ./packages/openland-web",
		"analyze:web": "BUNDLE_ANALYZE=browser next build ./packages/openland-web",
		"start": "NODE_ENV=production ./node_modules/.bin/ts-node --compilerOptions '{\"module\":\"commonjs\"}' packages/openland-web/server.ts ",
		"schema": "yarn schema:download && yarn schema:generate",
		"schema:local": "yarn schema:download:local && yarn schema:generate",
		"schema:download": "yarn apollo-codegen introspect-schema https://api.statecrafthq.com/api --output schema.json",
		"schema:download:local": "yarn apollo-codegen introspect-schema http://localhost:9000/api --output schema.json",
		"schema:generate": "yarn apollo-codegen generate ./packages/openland-api/queries/*.ts --schema schema.json --target typescript --output ./packages/openland-api/Types.ts --addTypename --tag-name gql",
		"promote": "kubectl set image deployment/openland-web app=`kubectl get deployment openland-web-next -o=jsonpath='{$.spec.template.spec.containers[:1].image}'` && kubectl rollout status deployments openland-web",
		"favicon-gen": "convert ./packages/openland-web/static/favicon-16.png ./packages/openland-web/static/favicon-32.png ./packages/openland-web/static/favicon-96.png ./packages/openland-web/static/favicon.ico"
	}
