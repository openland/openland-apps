	"scripts": {
		"dev": "BABEL_ENV=default API_WS_ENDPOINT=\"wss://api.openland.com/api\" API_ENDPOINT=\"https://api.openland.com\" nodemon ./packages/openland-web/server.ts",
		"dev:local": "BABEL_ENV=default API_WS_ENDPOINT=\"ws://localhost:9000/api\" nodemon ./packages/openland-web/server.ts",
		"lint": "BABEL_ENV=default NODE_ENV=production ./node_modules/.bin/tslint --project . --config ./tslint.json",
		"jest": "BABEL_ENV=default node ./node_modules/jest/bin/jest.js --no-cache",
		"jest:watch": "BABEL_ENV=default node ./node_modules/jest/bin/jest.js --watch",
		"test": "./node_modules/.bin/concurrently \"yarn lint\" \"yarn jest\" \"yarn test:ts\"",
		"test:ts": "BABEL_ENV=default NODE_ENV=production ./node_modules/.bin/tsc -p tsconfig.json --noemit",
		"bundle:app": "BABEL_ENV=default NODE_ENV=production next build ./packages/openland-web",
		"bundle:server": "BABEL_ENV=default NODE_ENV=production ./node_modules/.bin/tsc -p tsconfig.server.json",
		"bundle:prepare": "./node_modules/.bin/cpx \"./packages/openland-web/next.config.prod.js\" ./dist/ && ./node_modules/.bin/cpx \"./packages/openland-web/worker.js\" ./dist/ && mv ./dist/next.config.prod.js ./dist/next.config.js  && ./node_modules/.bin/cpx \"./packages/openland-web/.next/**/*\" ./dist/.next/ && ./node_modules/.bin/cpx \"./packages/openland-web/static/**/*\" ./dist/static/",
		"bundle": "./node_modules/.bin/concurrently \"yarn bundle:server\" \"yarn bundle:app\" && yarn bundle:prepare",
		"analyze": "BABEL_ENV=default NODE_ENV=production BUNDLE_ANALYZE=both next build ./packages/openland-web",
		"analyze:web": "BABEL_ENV=default NODE_ENV=production BUNDLE_ANALYZE=browser next build ./packages/openland-web",
		"analyze:server": "BABEL_ENV=default NODE_ENV=production BUNDLE_ANALYZE=server next build ./packages/openland-web",
		"inspect:build": "BABEL_ENV=default NODE_ENV=production next build ./packages/openland-web --inspect",
		"inspect:dev": "BABEL_ENV=default TS_NODE_PROJECT=tsconfig.server.json node --inspect -r ts-node/register ./packages/openland-web/server.ts",
		"native": "./node_modules/.bin/tsc -p tsconfig.json --outDir ./build/native && ./node_modules/.bin/concurrently \"yarn native:metro\" \"yarn native:build:watch\"",
		"native:metro": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
		"native:build": "./node_modules/.bin/tsc -p tsconfig.json --outDir ./build/native",
		"native:build:watch": "./node_modules/.bin/tsc -w -p tsconfig.json --outDir ./build/native",
		"native:ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator=\"iPhone X\"",
		"native:bundle": "./scripts/native_bundle.sh",
		"native:ios:logs": "node node_modules/react-native/local-cli/cli.js log-ios",
		"native:android": "node node_modules/react-native/local-cli/cli.js run-android",
		"schema": "yarn schema:download && yarn schema:generate",
		"schema:local": "yarn schema:download:local && yarn schema:generate",
		"schema:download": "yarn apollo-codegen introspect-schema https://api.statecrafthq.com/api --output schema.json",
		"schema:download:local": "yarn apollo-codegen introspect-schema http://localhost:9000/api --output schema.json",
		"schema:generate": "yarn schema:generate:1 && yarn schema:generate:2 && yarn schema:generate:3",
		"schema:generate:1": "yarn apollo-codegen generate ./packages/openland-api/*/*.ts --schema schema.json --target typescript --output ./packages/openland-api/Types.ts --addTypename --tag-name gql",
		"schema:generate:2": "./node_modules/.bin/ts-node --compilerOptions '{\"module\":\"commonjs\"}' ./packages/openland-y-graphql-gen/generator.ts",
		"schema:generate:3": "node ./fetch-fragments.js",
		"favicon-gen": "convert ./packages/openland-web/static/favicon-16.png ./packages/openland-web/static/favicon-32.png ./packages/openland-web/static/favicon-48.png ./packages/openland-web/static/favicon.ico",
		"postinstall": "patch-package"
	}
