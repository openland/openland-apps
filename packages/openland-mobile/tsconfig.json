{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "importHelpers": true,
      "jsx": "react-native",
      "lib": [
        "dom",
        "es2015",
        "es2016",
        "es2017"
      ],
      "module": "es2015",
      "moduleResolution": "node",
      "noEmitHelpers": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "outDir": "build/dist",
      "sourceMap": true,
      "strict": true,
      "target": "es2017"
    },
    "exclude": [
      "build",
      "node_modules"
    ],
    "types": [
      "typePatches"
    ]
  }