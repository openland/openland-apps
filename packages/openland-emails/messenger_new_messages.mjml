<mjml>
    <mj-head>
        <mj-title>You’ve got new messages</mj-title>
        <mj-include path="./attributes.mjml" />
    </mj-head>
    <mj-body background-color="#ffffff">
        <mj-include path="./header.mjml" />
        <mj-section>
            <mj-column>
                <mj-text mj-class="title">
                    You’ve got new messages
                </mj-text>

                <mj-raw>
                </mj-raw>
                    <mj-text mj-class="text">
                        There are {{messageCount}} new messages in your inbox. Go to Openland Messenger to see them now.
                    </mj-text>
                <mj-raw>
                </mj-raw>

                <mj-button mj-class="button" href="https://app.openland.com/mail">View messages</mj-button>
            </mj-column>
        </mj-section>
        <mj-include path="./footer-registered.mjml" />
    </mj-body>
</mjml>
