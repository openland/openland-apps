{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": [
      "es5",
      "es6",
      "dom",
      "esnext.asynciterable"
    ],
    "sourceMap": true,
    "allowJs": false,
    "jsx": "preserve",
    "moduleResolution": "node",
    "rootDir": "packages/openland-web",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": false,
    "strictPropertyInitialization": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "outDir": "dist/",
    "baseUrl": "packages",
    "importHelpers": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "packages/openland-web/src/setupTests.ts",
    "packages/openland-web/src/static",
    "packages/openland-web/src/less",
    "**/*.spec.ts"
  ],
  "files": [
    "./packages/openland-web/typings.d.ts",
    "./packages/openland-web/routes.ts",
    "./packages/openland-web/server.ts"
  ]
}