{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "es5",
      "es6",
      "dom",
      "esnext.asynciterable"
    ],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "packages",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "outDir": "lib/",
    "baseUrl": "packages",
    "paths": {
      "openland-api": ["openland-api"]
    },
    "importHelpers": true
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "packages/openland-web/src/setupTests.ts",
    "packages/openland-web/src/static",
    "packages/openland-web/src/less"
  ],
  "files": [
    "./packages/openland-web/typings.d.ts"
  ],
  "include": [
    "./packages/openland-web",
    "./packages/openland-api"
  ]
}